{
    "started": {
        "embed": {
            "color": 65296,
            "title": "De bot heeft een update ontvangen!",
            "description": "Je kunt me nu weer gebruiken :D",
            "timestamp": "{timestamp}"
        }
    },
    "stopping": {
        "embed": {
            "color": 13632027,
            "title": "De bot gaat afsluiten!",
            "description": "Hope to see you soon :(",
            "timestamp": "{timestamp}"
        }
    },
    "newInvite": {
        "embed": {
            "color": 5400599,
            "title": "Nieuwe invite aangemaakt!",
            "description": "Er is een nieuwe SV-turing invite aangemaakt",
            "fields": [
                {
                    "name": "Informatie:",
                    "value": "Deze link valt eenmalig te gebruiken, wees voorzichtig; deze link geeft meteen toestemming tot de sv-turing rank."
                },
                {
                    "name": "Link:",
                    "value": "{invite}"
                }
            ]
        }
    },
    "meme": {
        "embed": {
            "title": "{title}",
             "color": 29439,
            "footer": {
              "text": "Subreddit: {subreddit} | {postlink}"
            },
            "image": {
              "url": "{url}"
            }
          }
    }
}